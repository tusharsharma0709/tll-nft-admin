		<script src="plugins/custom/prismjs/prismjs.bundle.js"></script>
		<script src="js/scripts.bundle.js"></script>
		<!--end::Global Theme Bundle-->
		<!--begin::Page Vendors(used by this page)-->
		<script src="plugins/custom/fullcalendar/fullcalendar.bundle.js"></script>
		<script src="//maps.google.com/maps/api/js?key=AIzaSyBTGnKT7dt597vo9QgeQ7BFhvSRP4eiMSM"></script>
		<script src="plugins/custom/gmaps/gmaps.js"></script>
		<!--end::Page Vendors-->
		<!--begin::Page Scripts(used by this page)-->
		<script src="js/pages/widgets.js"></script>
        <script src="plugins/custom/datatables/datatables.bundle.js"></script>
		<script src="js/pages/features/miscellaneous/blockui.js"></script>
		<script src="js/custom/ellipsis.js"></script>
        <script src="js/custom/collectibles.js"></script>
		<!--end::Page Scripts-->
	</body>
	<!--end::Body-->
</html>