
<!DOCTYPE html>
<html lang="en">
	<!--begin::Head-->
	<%- include('../../partials/head'); %>
	<!--end::Head-->
	<!--begin::Body-->
	<body id="kt_body" class="header-fixed header-mobile-fixed subheader-enabled subheader-fixed aside-enabled aside-fixed aside-minimize-hoverable page-loading">
		<!--begin::Main-->
		<!--begin::Header Mobile-->
		<%- include('../../partials/mobileheader'); %>
		<!--end::Header Mobile-->
		<div class="d-flex flex-column flex-root">
			<!--begin::Page-->
			<div class="d-flex flex-row flex-column-fluid page">
				<!--begin::Aside-->
				<%- include('../../partials/aside'); %>
				<!--end::Aside-->
				<!--begin::Wrapper-->
				<div class="d-flex flex-column flex-row-fluid wrapper" id="kt_wrapper">
					<!--begin::Header-->
					<%- include('../../partials/header'); %>
					<!--end::Header-->
					<!--begin::Content-->
					<div class="content d-flex flex-column flex-column-fluid" id="kt_content">
						<!--begin::Subheader-->
						<%- include('../../partials/subheader'); %>
						<!--end::Subheader-->
						<!--begin::Entry-->
						<div class="d-flex flex-column-fluid">
							<!--begin::Container-->
							<div class="container">
                                	<!--begin::Card-->
								<div class="card card-custom">
									<div class="card-header flex-wrap border-0 pt-6 pb-0">
										<div class="card-title">
											<h3 class="card-label">Edit Collectible (<%= collectible_data.collectible_name  %>)</h3>
										</div>
                                        
										<!-- export option comes here -->		
									</div>
                                    <form id="editCollectible" name="editCollectible" method="POST" action="/postEditCollectible">
									<div class="card-body">
                                        <div class="form-group mb-8">
                                            <%- include ('../../partials/messages'); %>
                                        </div>
										<div class="form-group">
                                            <label>Collectible Name</label>
											<input type="hidden" name="collectible_uuid" value="<%= collectible_data.collectible_uuid  %>">
                                            <input type="text" class="form-control" value="<%= collectible_data.collectible_name  %>" disabled/>
                                        </div>
                                        <div class="form-group">
											<label for="exampleSelect2">Category
											<span class="text-danger">*</span></label>
											<select class="form-control" id="collectible_category" name="collectible_category" required>
												<option value="IRL Art" <%= collectible_data.collectible_category == 'IRL Art' ? 'selected' : ''%>>IRL Art</option>
												<option value="Sora" <%= collectible_data.collectible_category == 'Sora' ? 'selected' : ''%>>Sora</option>
												<option value="Art" <%= collectible_data.collectible_category == 'Art' ? 'selected' : ''%>>Art</option>
												<option value="Animations" <%= collectible_data.collectible_category == 'Animations' ? 'selected' : ''%>>Animations</option>
												<option value="Manga" <%= collectible_data.collectible_category == 'Manga' ? 'selected' : ''%>>Manga</option>
												<option value="Memes" <%= collectible_data.collectible_category == 'Memes' ? 'selected' : ''%>>Memes</option>
												<option value="Trading Cards" <%= collectible_data.collectible_category == 'Trading Cards' ? 'selected' : ''%>>Trading Cards</option>
												<option value="Collectibles" <%= collectible_data.collectible_category == 'Collectibles' ? 'selected' : ''%>>Collectibles</option>
												<option value="Unique" <%= collectible_data.collectible_category == 'Unique' ? 'selected' : ''%>>Unique</option>
												<option value="NSFW" <%= collectible_data.collectible_category == 'NSFW' ? 'selected' : ''%>>NSFW</option>
												<option value="Audio" <%= collectible_data.collectible_category == 'Audio' ? 'selected' : ''%>>Audio</option>
												<option value="Video" <%= collectible_data.collectible_category == 'Video' ? 'selected' : ''%>>Video</option>
												<option value="3D Model" <%= collectible_data.collectible_category == '3D Model' ? 'selected' : ''%>>3D Model</option>
												<option value="SPORKDAO" <%= collectible_data.collectible_category == 'SPORKDAO' ? 'selected' : ''%>>SPORKDAO</option>
												<option value="BLMF" <%= collectible_data.collectible_category == 'BLMF' ? 'selected' : ''%>>BLMF</option>
												<option value="ArtSapotRakyat" <%= collectible_data.collectible_category == 'ArtSapotRakyat' ? 'selected' : ''%>>ArtSapotRakyat</option>
											</select>
										</div>
                                        <div class="form-group">
                                            <label>Locked Content Status
                                            <span class="text-danger">*</span></label>
                                            <select class="form-control" name="collectible_lock_status" id="collectible_lock_status" required>
												<option value="1" <%= collectible_data.lock_status == '1' ? 'selected' : ''%>>Yes</option>
												<option value="0" <%= collectible_data.lock_status == '0' ? 'selected' : ''%>>No</option>
											</select>
                                        </div>
										<div class="form-group" id="locked_content_div">
                                            <label>Provide the Links of the content which buyer can download, post purchase
                                            <span class="text-danger">*</span></label>
                                            <textarea class="form-control" name="locked_content" id="locked_content" rows="3"><%= collectible_data.locked_content  %></textarea>
                                        </div>
                                    </div>
                                    <div class="card-footer">
                                        <button type="reset" class="btn btn-secondary">Cancel</button>
                                        <button type="submit" class="btn btn-primary mr-2">Submit</button>
                                    </div>
                                    </form>

								</div>
								<!--end::Card-->
							</div>
							<!--end::Container-->
                        </div>
						<!--end::Entry-->
					</div>
					<!--end::Content-->
					<!--begin::Footer-->
					<%- include('../../partials/footer'); %>
					<!--end::Footer-->
				</div>
				<!--end::Wrapper-->
			</div>
			<!--end::Page-->
		</div>
		<!--end::Main-->
		<!-- begin::User Panel-->
		<%- include('../../partials/userpanel'); %>
		<!-- end::User Panel-->
		
		<!--begin::Scrolltop-->
		<div id="kt_scrolltop" class="scrolltop">
			<span class="svg-icon">
				<!--begin::Svg Icon | path:media/svg/icons/Navigation/Up-2.svg-->
